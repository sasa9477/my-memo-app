{
  "name": "my-memo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "dev": "npm run clean && tsc -p tsconfig.server.json && node ./dist/main.js",
    "build:next": "next build",
    "build:server": "npm run clean && tsc -p tsconfig.server.json",
    "start": "NODE_ENV=production node dist/main.js",
    "lint": "next lint",
    "prisma-init": "prisma db push",
    "prisma-generate": "prisma generate",
    "aspida:clean": "rimraf ./src/api",
    "aspida:generate": "openapi2aspida -i ./src/openapi.yaml -o ./src/api",
    "openapi-generate": "run-s aspida:clean aspida:generate"
  },
  "dependencies": {
    "@aspida/axios": "^1.8.0",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.3",
    "@prisma/client": "^3.10.0",
    "axios": "^0.26.0",
    "date-fns-tz": "^1.2.2",
    "express": "^4.17.3",
    "next": "12.1.0",
    "next-auth": "^4.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.27.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "npm-run-all": "^4.1.5",
    "openapi2aspida": "^0.17.0",
    "prisma": "^3.10.0",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.3.0",
    "typescript": "4.5.5"
  }
}
